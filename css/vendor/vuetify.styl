// Specify overrides (theme and/or base variables etc.)
// See https://vuetifyjs.com/quick-start
@import '~vuetify/src/stylus/settings/_colors'

$theme := {
  primary: $grey.darken-4
  accent: $green.darken-3
  secondary: $grey.darken-2
  info: #0D47A1
  warning: #ffb300
  error: #B71C1C
  success: #2E7D32
}

// Import Vuetify styling
@require '~vuetify/src/stylus/main.styl'
